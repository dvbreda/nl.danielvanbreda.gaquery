{
  "name": {
    "en": "Google Assistant Driver"
  },
  "class": "other",
  "capabilities": [
    "last_query",
    "last_response",
    "last_device_action"
  ],
  "capabilitiesOptions": {
    "last_query": {
      "title": {
        "en": "Last Query",
        "nl": "Laatste Vraag"
      },
      "getable": true,
      "setable": false
    },
    "last_response": {
      "title": {
        "en": "Last Response",
        "nl": "Laatste Antwoord"
      },
      "getable": true,
      "setable": false
    },
    "last_device_action": {
      "title": {
        "en": "Last Device Action",
        "nl": "Laatste Apparaat Actie"
      },
      "getable": true,
      "setable": false
    }
  },
  "platforms": [
    "local"
  ],
  "connectivity": [
    "cloud"
  ],
  "images": {
    "small": "{{driverAssetsPath}}/images/small.png",
    "large": "{{driverAssetsPath}}/images/large.png",
    "xlarge": "{{driverAssetsPath}}/images/xlarge.png"
  },
    "id": "my_driver",
    "pair": [
      {
        "id": "login_oauth2",
        "template": "login_oauth2"
      },
      {
        "id": "list_devices",
        "template": "list_devices",
        "navigation": {
          "next": "add_devices"
        }
      },
      {
        "id": "add_devices",
        "template": "add_devices"
      }
    ],
    "repair": [
      {
        "id": "login_oauth2",
        "template": "login_oauth2"
      }
    ]
  }