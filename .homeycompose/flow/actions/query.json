{
  "title": {
    "en": "Query",
    "nl": "Vraag"
  },
  "id": "query",
  "hint": {
    "en": "Send a query to Google Assistant",
    "nl": "Stuur een vraag naar Google Assistant"
  },
  "titleFormatted": {
    "en": "Ask [[query]] to Google Assistant on [[speaker]]. New conversation: [[new]]",
    "nl": "Vraag [[query]] naar Google Assistant op [[speaker]]. Nieuw gesprek: [[new]]"
  },
  "args": [
    {
      "type": "text",
      "name": "query",
      "placeholder": {
        "en": "query",
		"nl": "vraag"
      }
    },
    {
      "type": "autocomplete",
      "name": "speaker",
      "title": {
        "en": "Speaker/Display",
        "nl": "Speaker/Scherm"
      },
      "placeholder": {
        "en": "Select speaker...",
        "nl": "Selecteer speaker..."
      }
    },
    {
      "type": "checkbox",
      "name": "new",
      "placeholder": {
        "en": "New conversation?",
		"nl": "Nieuw gesprek?"
      }
    }
  ],
  "tokens": [
    {
      "name": "response",
      "type": "string",
      "title": {
        "en": "Response",
        "nl": "Antwoord"
      },
      "example": {
        "en": "The weather is sunny today",
        "nl": "Het weer is zonnig vandaag"
      }
    },
    {
      "name": "device_action",
      "type": "string",
      "title": {
        "en": "Device Action",
        "nl": "Apparaat Actie"
      },
      "example": {
        "en": "{\"type\":\"turn_on\",\"device\":\"lights\"}",
        "nl": "{\"type\":\"turn_on\",\"device\":\"lights\"}"
      }
    }
  ]
}